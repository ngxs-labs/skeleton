{
  "$schema": "../node_modules/ng-packagr/package.schema.json",
  "name": "@ngxs-labs/skeleton",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ngxs-labs/skeleton.git"
  },
  "license": "MIT",
  "homepage": "https://github.com/ngxs-labs/skeleton#readme",
  "bugs": {
    "url": "https://github.com/ngxs-labs/skeleton/issues"
  },
  "keywords": [
    "ngxs",
    "redux",
    "store"
  ],
  "sideEffects": true,
  "peerDependencies": {
    "@angular/core": ">=7.0.0",
    "@ngxs/store": ">=5.0.0",
    "rxjs": ">=6.5.2"
  },
  "ngPackage": {
    "lib": {
      "flatModuleFile": "ngxs-labs-skeleton",
      "entryFile": "index.ts",
      "umdModuleIds": {
        "@ngxs/store": "ngxs-store"
      }
    },
    "dest": "../dist/skeleton"
  }
}
